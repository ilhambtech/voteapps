pipeline {
    agent {
        kubernetes {
            containerTemplate {
                name 'maven'
                image 'docker:dind'
                command 'sleep'
                args '99d'
                privileged true
            }
        }
    }

    stages {
        stage('Run docker') {
            steps {
                container('maven'){
                    sh """
                        
                        cat /etc/os-release;
                        ls -lah;
                        cd vote;
                        which docker;
                        docker version;
                    """
                }
                
            }
        }
    }
}
